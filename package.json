{
	"name": "synthesizer-v-script-helper",
	"displayName": "Synthesizer V Script Helper",
	"description": "SynthV脚本编写助手",
	"keywords": [
		"SynthV",
		"Synthesizer V"
	],
	"preview": true,
	"publisher": "linyuansup",
	"icon": "svcoding.png",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.27.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:synthesizer-v-script-helper.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "lua",
				"path": "./snippets/lua.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/jsandlua.json"
			},
			{
				"language": "lua",
				"path": "./snippets/jsandlua.json"
			}
		],
		"commands": [
			{
				"command": "synthesizer-v-script-helper.helloWorld",
				"title": "SynthV"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.27.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
